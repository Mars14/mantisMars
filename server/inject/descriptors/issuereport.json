{
  "type": "object",
  "comment": "report",
  "properties": {
    "camera": {
      "type": "string",
      "selector": "[id*=\"ufile\"]",
      "control": "text"
    },
    "Category": {
      "type": "select",
      "selector": "[name=\"category_id\"]",
      "control": "select"
    },
    "description": {
      "type": "string",
      "update": {
        "type": "none"
      },
      "selector": "[name=\"description\"]",
      "control": "text"
    },
    "submit": {
      "type": "action",
      "selector": ".button[type=\"submit\"]",
      "control": "action.button"
    },
    "summary": {
      "type": "string",
      "selector": "[name=\"summary\"]",
      "control": "property.innerText"
    },
    "isIssueReport": {
      "type": "identifier",
      "selector": ".button[value='Submit Report']",
      "control": "exists"
    }
  },
  "connect": [
    {
      "match": [
        "isIssueReport"
      ],
      "screens": [
        "issuereport"
      ]
    }
  ]
}
